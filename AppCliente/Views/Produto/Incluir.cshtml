@model ProdutoModel

@{
    ViewData["Title"] = "Incluir Produto";
}

<h1>Incluir Produto</h1>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Incluir">

            <partial name="_CamposPreenchimentoProduto" />

            <div class="form-group">
                <input type="submit" value="Incluir" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Retornar a lista de Produtos</a>
</div>

